@misc{tanzilliHighlyEfficientPhotonpair2000,
  title = {Highly Efficient Photon-Pair Source Using a {{Periodically Poled Lithium Niobate Waveguide}}},
  author = {Tanzilli, S. and de Riedmatten, H. and Tittel, W. and Zbinden, H. and Baldi, P. and de Micheli, M. and Ostrowsky, D. B. and Gisin, N.},
  year = 2000,
  month = dec,
  number = {arXiv:quant-ph/0012053},
  eprint = {quant-ph/0012053},
  publisher = {arXiv},
  doi = {10.48550/arXiv.quant-ph/0012053},
  urldate = {2025-09-03},
  abstract = {We report on a new kind of correlated photon-pair source based on a waveguide integrated on a Periodically Poled Lithium Niobate substrate. Using a pump laser of a few micro-Watts at 657 nm, we generate degenerate photon-pairs at 1314 nm. Detecting about 1500 coincidences per second, we can infer a conversion rate of 10-6 pairs per pump photon, which is four orders of magnitude higher than that obtained with previous bulk sources. These results are very promising for the realization of sources for quantum communication and quantum metrology experiments requiring a high signal-to-noise ratio or working with more than one photon-pair at a time.},
  archiveprefix = {arXiv},
  keywords = {CrystalSPDC,Quantum Physics},
  file = {C\:\\Users\\goesswein\\Downloads\\ZoteroPortable\\Data\\profile\\storage\\D2XLXPTW\\Tanzilli et al. - 2000 - Highly efficient photon-pair source using a Periodically Poled Lithium Niobate Waveguide.pdf;C\:\\Users\\goesswein\\Downloads\\ZoteroPortable\\Data\\profile\\storage\\U5ZU23F4\\0012053.html}
}

