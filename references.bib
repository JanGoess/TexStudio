@misc{maHighlyEfficientPure2023,
  title = {A {{Highly Efficient}} and {{Pure Few-Photon Source}} on {{Chip}}},
  author = {Ma, Zhaohui and Chen, Jia-Yang and Garikapati, Malvika and Li, Zhan and Tang, Chao and Sua, Yong Meng and Huang, Yu-Ping},
  year = 2023,
  month = sep,
  number = {arXiv:2309.15233},
  eprint = {2309.15233},
  primaryclass = {quant-ph},
  publisher = {arXiv},
  doi = {10.48550/arXiv.2309.15233},
  urldate = {2025-08-28},
  abstract = {We report on multi-photon statistics of correlated twin beams produced in a periodic poled micro-ring resonator on thin-film lithium niobate. Owing to high cavity confinement and near perfect quasi-phase matching, the photons pairs are produced efficiently in single modes at rates reaching 27 MHz per \${\textbackslash}mu\$W pump power. By using a pump laser whose pulse width impedance matches with the cavity, those photons are further created in single longitudinal modes with purity reaching 99{\textbackslash}\%, without relying on later-on filtering. With a dual-channel photon-number resolving detection system, we obtain directly the joint detection probabilities of multi-photon states up to three photons, with high coincidence to accidental contrast for each. Used as a single photon source, it gives heralded \$g\_H{\textasciicircum}\{(2)\}(0)\$ around 0.04 at a single photon rate of 650 kHz on chip. The findings of our research highlight the potential of this nanophotonic platform as a promising platform for generating non-classical, few-photon states with ideal indistinguishability, for fundamental quantum optics studies and information applications.},
  archiveprefix = {arXiv},
  keywords = {Quantum Physics},
  file = {C\:\\Users\\goesswein\\Downloads\\ZoteroPortable\\Data\\profile\\storage\\5P5ZRSA5\\Ma et al. - 2023 - A Highly Efficient and Pure Few-Photon Source on Chip.pdf;C\:\\Users\\goesswein\\Downloads\\ZoteroPortable\\Data\\profile\\storage\\8UJV3UCW\\2309.html}
}

